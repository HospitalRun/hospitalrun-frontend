{{#x-list name="info-list" schema=(schema-for "info-list") as |list|}}
  <tbody>
    {{#each model as |library|}}
      <tr class="list__row js-library-row">
        {{#list.cell class="list__cell_main"}}
          <span class="js-lib-name">{{library.name}}</span>
        {{/list.cell}}
        {{#list.cell}}
          <span class="js-lib-version">{{library.version}}</span>
        {{/list.cell}}
      </tr>
    {{/each}}
  </tbody>
{{/x-list}}
