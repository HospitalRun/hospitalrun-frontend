        <path class="svg-stroke" shape-rendering="crispEdges" d="M10.5,0 L10.5,12" stroke="#000000"></path>
        <path class="svg-fill" d="M6.25,4 L9.25,9.5 L3.25,9.5 L6.25,4 Z" fill="#000" transform="translate(6.250000, 6.500000) scale(-1, 1) rotate(-90.000000) translate(-6.250000, -6.500000) "></path>
      </g>
    </svg>
  {{else}}
    <svg width="16px" height="14px" viewBox="0 0 16 14" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
      <title>Expand Left Sidebar</title>
      <g id="expand-sidebar-left" stroke="none" fill="none" transform="translate(8.000000, 8.000000) scale(-1, 1) translate(-8.000000, -7.000000)">
        <rect class="svg-stroke" stroke="#000000" x="0.5" y="0.5" width="14" height="12"></rect>
        <path class="svg-stroke" shape-rendering="crispEdges" d="M10.5,0 L10.5,12" stroke="#000000"></path>
