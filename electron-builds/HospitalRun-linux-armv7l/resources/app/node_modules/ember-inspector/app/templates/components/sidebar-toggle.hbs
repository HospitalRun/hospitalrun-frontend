{{#if isRight}}
  {{#if isExpanded}}
    <svg width="16px" height="14px" viewBox="0 0 16 14" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
      <title>Collapse Right Sidebar</title>
      <g id="expand-sidebar-left" stroke="none" fill="none" transform="translate(0,1)">
        <rect class="svg-stroke" stroke="#000000" x="0.5" y="0.5" width="14" height="12"></rect>
        <path class="svg-stroke" shape-rendering="crispEdges" d="M10.75,0 L10.75,12" stroke="#000000"></path>
        <path class="svg-fill" d="M6.25,4 L9.25,9.5 L3.25,9.5 L6.25,4 Z" fill="#000" transform="translate(6.250000, 6.500000) scale(-1, 1) rotate(-90.000000) translate(-6.250000, -6.500000) "></path>
      </g>
    </svg>
  {{else}}
    <svg width="16px" height="14px" viewBox="0 0 16 14" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
      <title>Expand Right Sidebar</title>
      <g id="expand-sidebar-left" stroke="none" fill="none" transform="translate(0,1)">
        <rect class="svg-stroke" stroke="#000000" x="0.5" y="0.5" width="14" height="12"></rect>
        <path class="svg-stroke" shape-rendering="crispEdges" d="M10.75,0 L10.75,12" stroke="#000000"></path>
        <path class="svg-fill" d="M5.25,4 L8.25,9.25 L2.25,9.25 L5.25,4 L5.25,4 Z" fill="#000000" transform="translate(5.250000, 6.500000) rotate(-90.000000) translate(-5.250000, -6.500000)"></path>
      </g>
    </svg>
  {{/if}}
{{else}}
  {{#if isExpanded}}
    <svg width="16px" height="14px" viewBox="0 0 16 14" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
