{{#modal-dialog
    cancelAction='closeModal'
    isUpdateDisabled=isUpdateDisabled
    title=(t 'customForms.titles.addCustomForm')
    updateButtonAction='addForm'
    updateButtonText=(t 'customForms.buttons.addForm')}}
  {{#em-form model=model submitButton=false}}
    {{em-select label=(t 'customForms.labels.formToAdd')
      property="selectedForm" content=model.customForms
      prompt=" "
    }}
  {{/em-form}}
{{/modal-dialog}}
